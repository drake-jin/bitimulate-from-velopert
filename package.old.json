{
  "name": "koa.js",
  "version": "1.0.0",
  "description": "이것은 그냥 내 스터디용",
  "main": "index.js",
  "scripts": {
    "start:dev": "NODE_PATH=src nodemon --watch src/ src/index.js",
    "crawler:dev": "NODE_PATH=src nodemon --watch src/crawler/ src/crawler/index.js",
    "test": "jest --forceExit",
    "test-cov": "npm run test -- --coverage",
    "lint": "eslint benchmarks lib test --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_ENV=production node app",
    "dev": "NODE_ENV=development nodemon --inspect app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drake-jin/koa.js.git"
  },
  "keywords": [
    "koa.js",
    "javascript",
    "rest",
    "api"
  ],
  "author": "drake-jin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drake-jin/koa.js/issues"
  },
  "homepage": "https://github.com/drake-jin/koa.js#readme",
  "dependencies": {
    "keygrip": "^1.0.1",
    "axios": "^0.16.2",
    "dotenv": "^4.0.0",
    "koa": "^2.3.0",
    "koa-router": "^7.2.1",
    "mongoose": "^4.11.4",
    "mongoose-double": "^0.0.1",
    "ws": "^3.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.17.1",
    "eslint-config-standard": "^7.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "istanbul": "^0.4.0",
    "jest": "^20.0.0",
    "supertest": "^3.0.0"
  },
  "jest": {
    "testMatch": [
      "**/test/!(helpers)/*.js"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov"
    ],
    "bail": true,
    "testEnvironment": "node"
  }
}
